{
    "manifest_version": 3,
    "version": "1.3",
    "name": "Retro Computer Sounds",
    "description": "Just some fun retro computer sounds",
    "developer": {
        "name": "Evan Tanner"
    },
    "icons": {
        "512": "art.png"
    },
    "mod": {
        "license": "license.txt",
        "schema_version": 1,
        "payload": {
            "browser_sounds": {
                "CLICK": [
                    "sounds/click.mp3"
                ],
                "FEATURE_SWITCH_OFF": [
                    "sounds/floppy_remove.mp3"
                ],
                "FEATURE_SWITCH_ON": [
                    "sounds/floppy_insert.mp3"
                ],
                "IMPORTANT_CLICK": [
                    "sounds/click.mp3"
                ],
                "LIMITER_OFF": [
                    "sounds/off.mp3"
                ],
                "LIMITER_ON": [
                    "sounds/on.mp3"
                ],
                "HOVER": [
                    "sounds/drive_read1.mp3",
                    "sounds/drive_read2.mp3",
                    "sounds/drive_read3.mp3"
                ],
                "HOVER_UP": [
                    "sounds/drive_read1.mp3",
                    "sounds/drive_read2.mp3",
                    "sounds/drive_read3.mp3"
                ],
                "SWITCH_TOGGLE": [
                    "sounds/switch1.mp3",
                    "sounds/switch2.mp3",
                    "sounds/switch3.mp3",
                    "sounds/switch4.mp3"
                ],
                "TAB_CLOSE": [
                    "sounds/floppy_read1.mp3",
                    "sounds/floppy_read2.mp3",
                    "sounds/floppy_read3.mp3"
                ],
                "TAB_INSERT": [
                    "sounds/floppy_read1.mp3",
                    "sounds/floppy_read2.mp3",
                    "sounds/floppy_read3.mp3"
                ],
                "TAB_SLASH": [
                    "sounds/beep.mp3"
                ]
            },
            "keyboard_sounds": {
                "TYPING_BACKSPACE": [
                    "keyboard/backspace.mp3"
                ],
                "TYPING_ENTER": [
                    "keyboard/enter.mp3"
                ],
                "TYPING_LETTER": [
                    "keyboard/keyboard1.mp3",
                    "keyboard/keyboard2.mp3",
                    "keyboard/keyboard3.mp3",
                    "keyboard/keyboard4.mp3"
                ],
                "TYPING_SPACE": [
                    "keyboard/spacebar.mp3"
                ]
            }
        }
    }
}